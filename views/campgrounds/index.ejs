<% layout('layouts/boilerplate') %>
    <h1>All campgrounds</h1>
    <h4><a href="/campgrounds/new">New campground</a></h4>
    <ul>
        <% for(let campground of campgrounds){%>
            <li>
                <a href="/campgrounds/<%= campground._id %> ">
                    <%= campground.title %>
                </a>
            </li>
            <%} %>
    </ul>